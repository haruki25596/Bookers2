<%= render 'users/list', user: @user, book: @book %>

<div class='col-md-8 offset-md-1'>
      <h2>Users</h2>
      <table class='table'>

<thead>
 <tr>
  <th>image</th>
  <th>name</th>
   <th colspan="3"></th>
 </tr>
  </thead>
  <tbody>
    <tr class="users">
      <% @users.each do |user| %>
      <th><%= attachment_image_tag user, :profile_image, :fill, 50, 50, size: "50x50", fallback: "no_image.jpeg" %></th>
      <th><%= user.name %></th>
      <th><%= link_to "Show", user_path(user) %></th>
      <th colspan="3"></th>
    </tr>
  <% end %>

  </tbody>
</div>